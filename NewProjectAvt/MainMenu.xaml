            <TextBlock Canvas.Left="37" TextWrapping="Wrap" Text="Логин" Canvas.Top="178" Width="89" Height="15"/>
            <TextBlock Canvas.Left="37" TextWrapping="Wrap" Text="Пароль" Canvas.Top="254" Width="89" Height="15"/>
        </Canvas>
        <TextBox x:Name="tblastname" Height="41" Canvas.Left="36" TextWrapping="Wrap" Text="TextBox" Canvas.Top="126" Width="252"/>
        <TextBox x:Name="tblogin" Height="41" Canvas.Left="36" TextWrapping="Wrap" Text="TextBox" Canvas.Top="201" Width="252"/>
        <TextBox x:Name="tbpassword" Height="41" Canvas.Left="36" TextWrapping="Wrap" Text="TextBox" Canvas.Top="275" Width="252"/>
    </Canvas>
</Window>
